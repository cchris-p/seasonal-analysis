{
    "include": [
        "seasonal_analysis"
    ],
    "exclude": [
        "**/__pycache__",
        "**/.venv",
        "**/.git",
        "**/node_modules",
        "build",
        "dist",
        "*.egg-info",
        "study",
        "docs",
        "results"
    ],
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.10",
            "extraPaths": [
                "."
            ]
        }
    ],
    "typeCheckingMode": "standard",
    "reportMissingImports": true,
    "reportMissingTypeStubs": false
}